{
    "name": "tallsaas/obsidian",
    "description": "Obsidian templating language support for TALLSaaS tools.",
    "keywords": [
        "obsidian",
        "saas",
        "tailwind",
        "alpinejs",
        "laravel",
        "livewire"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Anderton",
            "email": "hey@joshuaanderton.ca"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.67|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Obsidian\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Obsidian\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obsidian\\Providers\\ObsidianServiceProvider"
            ]
        }
    }
}